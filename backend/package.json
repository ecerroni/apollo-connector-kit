{
  "name": "apollo-backend",
  "version": "0.1.0",
  "description": "",
  "main": "./src/server.js",
  "scripts": {
    "lint": "eslint .",
    "start": "lookenv --path=./env/lookenv.dev.js --dotenv=./env/.env -- nodemon ./src/server.js --exec babel-node -r './node_modules/dotenv/config' -e js",
    "start-docker": "bash start-docker.sh",
    "add-component-part": "cd ./src/scripts && bash create-graphql-component.sh",
    "remove-component-part": "cd ./src/scripts && bash delete-graphql-component.sh",
    "build": "babel -d ./build ./src -s",
    "serve-staging": "NODE_ENV=staging node ./server.js",
    "serve-production": "NODE_ENV=production node -r 'node_modules/dotenv/config' ./server.js",
    "serve-testing": "babel-node ./src/server.js",
    "start-jest": "jest",
    "test": "lookenv --path=./env/lookenv.dev.js --dotenv=./env/.env -- yarn start-test serve-testing 3000 start-jest"
  },
  "author": "",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-proposal-async-generator-functions": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "23.6.0",
    "babel-plugin-bulk-import": "2.0.0",
    "babel-plugin-root-import": "6.1.0",
    "dotenv": "6.2.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "^5.1.0",
    "eslint-friendly-formatter": "3.0.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-graphql": "3.0.3",
    "eslint-plugin-html": "4.0.2",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-prettier": "^3.1.0",
    "graphql-tester-options": "0.0.6",
    "jest": "24.5.0",
    "jwt-decode": "2.2.0",
    "nodemon": "1.18.7",
    "prettier": "^1.18.2",
    "start-server-and-test": "1.7.11"
  },
  "dependencies": {
    "@babel/runtime": "7.2.0",
    "@okgrow/graphql-scalars": "0.4.5",
    "apollo-server-express": "2.4.8",
    "apollo-server-module-operation-store": "1.3.5",
    "await-to-js": "^2.1.1",
    "bcrypt": "3.0.3",
    "cookie-parser": "1.4.3",
    "cookie-session": "2.0.0-beta.3",
    "cors": "2.8.4",
    "express": "4.16.2",
    "graphql": "14.1.1",
    "graphql-cost-analysis": "1.0.3",
    "graphql-depth-limit": "1.1.0",
    "graphql-input-number": "0.0.10",
    "graphql-input-string": "0.0.16",
    "graphql-tools": "4.0.3",
    "graphql-type-json": "^0.3.0",
    "jsonwebtoken": "8.1.1",
    "lodash.mapvalues": "^4.6.0",
    "lookenv": "1.0.1",
    "merge-graphql-schemas": "^1.5.7",
    "uuid": "3.3.2"
  },
  "license": "MIT"
}
